{"_id":"o2SRHA7NgeXl3HKR","name":"Lyron Cham","type":"character","img":"modules/alienrpg-starterset/images/avatar/lyron-cham-avatar.png","items":[{"_id":"kcUU942E4Jc9yS1w","name":"True Grit","type":"talent","sort":100000,"flags":{},"img":"icons/svg/mystery-man.svg","effects":[],"folder":null,"system":{"attributes":{"flag":{"value":0,"label":"Flag"}},"general":{"career":{"value":"10","label":"career"},"comment":{"value":"Life on the Frontier is a constant struggle. Luckily, you have what it takes to overcome anything that comes your way. You can push any skill roll based on Strength twice, not just once like other characters. Each push increases your Stress Level by one.","label":"Text"}},"notes":{"notes":""}},"ownership":{"default":0},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}}],"effects":[],"folder":"v5ochSwdWr8xFVto","sort":0,"flags":{"core":{"sourceId":"Compendium.alienrpg-starterset.alienrpg-cotg-pcs.pM9v0lNPUs5LRQN4"}},"system":{"header":{"health":{"value":5,"label":"Health","mod":0,"max":5,"calculatedMax":5},"stress":{"value":0,"label":"Stress","mod":0,"max":10},"npc":false},"attributes":{"str":{"value":5,"label":"Strength","mod":5,"proficient":0},"wit":{"value":2,"label":"Wits","mod":2},"agl":{"value":3,"label":"Agility","mod":3},"emp":{"value":4,"label":"Empathy","mod":4}},"skills":{"heavyMach":{"value":3,"label":"Heavy Machinery","ability":"str","description":"Heavy Machinery","mod":8},"closeCbt":{"value":2,"label":"Close Combat","ability":"str","description":"Close Combat","mod":7},"stamina":{"value":2,"label":"Stamina","ability":"str","description":"Stamina","mod":7},"rangedCbt":{"value":0,"label":"Ranged Combat","ability":"agl","description":"Ranged Combat","mod":3},"mobility":{"value":0,"label":"Mobility","ability":"agl","description":"Mobility","mod":3},"piloting":{"value":0,"label":"Piloting","ability":"agl","description":"Piloting","mod":3},"command":{"value":0,"labe